<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>



<ul>
  <li> Name: <%= @user.name %> </li>
  <li> Email: <%= @user.email %> </li>
  <li> created_at: <%= @user.created_at %> </li>
  <li> updated_at: <%= @user.updated_at %> </li>
  <li> Time.now: <%= Time.now %> </li>
</ul>
